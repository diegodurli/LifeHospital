<div class="panel-group" id="accordion">
  <%= render partial: 'partials/panel_collapse', locals: {id: 'personalData', resource: 'Personal Data', panelDataPartial: 'partials/personal_data'              , panelDataLocals: {patient: patient}} %>
  <%= render partial: 'partials/panel_collapse', locals: {id: 'hospitalizations', resource: 'Hospitalizations', panelDataPartial: 'partials/list_resource_array' , panelDataLocals: {resource: 'Hospitalizations' , records: get_details_for_patient(patient,'Hospitalizations')}} %>
  <%= render partial: 'partials/panel_collapse', locals: {id: 'clinicalOutcomes', resource: 'Clinical Outcomes', panelDataPartial: 'partials/list_resource_array', panelDataLocals: {resource: 'Clinical Outcomes', records: get_details_for_patient(patient,'Clinical Outcomes')}} %>
  <%= render partial: 'partials/panel_collapse', locals: {id: 'exams', resource: 'Exams', panelDataPartial: 'partials/list_resource_array'                       , panelDataLocals: {resource: 'Exams'            , records: get_details_for_patient(patient,'Exams')}} %>
  <%= render partial: 'partials/panel_collapse', locals: {id: 'medicaments', resource: 'Medicaments', panelDataPartial: 'partials/list_resource_array'           , panelDataLocals: {resource: 'Medicaments'      , records: get_details_for_patient(patient,'Medicaments')}} %>
  <%= render partial: 'partials/panel_collapse', locals: {id: 'procedures', resource: 'Procedures', panelDataPartial: 'partials/list_resource_array'             , panelDataLocals: {resource: 'Procedures'       , records: get_details_for_patient(patient,'Procedures')}} %>
  <%= render partial: 'partials/panel_collapse', locals: {id: 'diets', resource: 'Diets', panelDataPartial: 'partials/list_resource_array'                       , panelDataLocals: {resource: 'Diets'            , records: get_details_for_patient(patient,'Diets')}} %>
  <%= render partial: 'partials/panel_collapse', locals: {id: 'prescriptions', resource: 'Prescriptions', panelDataPartial: 'partials/list_resource_array'       , panelDataLocals: {resource: 'Prescriptions'    , records: get_details_for_patient(patient,'Prescriptions')}} %>
  <%= render partial: 'partials/panel_collapse', locals: {id: 'specialCares', resource: 'Special Cares', panelDataPartial: 'partials/list_resource_array'        , panelDataLocals: {resource: 'Special Cares'    , records: get_details_for_patient(patient,'Special Cares')}} %>
</div>